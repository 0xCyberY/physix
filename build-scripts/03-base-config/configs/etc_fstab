/dev/mapper/VOL_GROUP_MARKER-root /      FS_MARKER subvol=physix_stack-0 defaults   0 1
UUID=BOOT_UUID_MARKER   /boot  ext2   defaults   0 2
/dev/mapper/VOL_GROUP_MARKER-home /home  FS_MARKER  defaults   0 2  
/dev/mapper/VOL_GROUP_MARKER-var  /var   FS_MARKER  defaults   0 2
#/dev/mapper/VOL_GROUP_MARKER-usr  /usr   FS_MARKER  defaults   0 2
/dev/mapper/VOL_GROUP_MARKER-opt  /opt   FS_MARKER  defaults   0 2
/dev/mapper/VOL_GROUP_MARKER-tmp  /tmp   FS_MARKER  defaults   0 2
#/dev/REPLACE_MARKER_SWP     swap         swap     pri=1               0     0

# End /etc/fstab

