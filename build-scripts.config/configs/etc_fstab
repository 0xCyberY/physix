
# Begin /etc/fstab

# file system  mount-point  type     options             dump  fsck
#                                                              order

/dev/mapper/physix-root /      FS_MARKER  defaults   0 1
UUID=BOOT_UUID_MARKER   /boot  ext2   defaults   0 2
/dev/mapper/physix-home /home  FS_MARKER  defaults   0 2  
/dev/mapper/physix-var  /var   FS_MARKER  defaults   0 2
/dev/mapper/physix-usr  /var   FS_MARKER  defaults   0 2
#/dev/REPLACE_MARKER_SWP     swap         swap     pri=1               0     0

# End /etc/fstab

